<!DOCTYPE html>
<html>
  <head>
    <title><%= @title || "HomepointrCIC" %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application' %>

    <%= favicon_link_tag asset_path('logo.png'), :rel => 'icon', :type =>  'image/png' %>

  </head>

  <body>
    <% if current_user != nil %>
      <% if current_user.type == "ReferralAgency"%>
        <%= react_component("individual/NavBar", props={ id: current_user.id, type: "referral_agencies", email: current_user.email }) %>
      <% else %>
        <%= react_component("individual/NavBar", props={ id: current_user.id, type: "landlords", email: current_user.email }) %>
      <%end%>
    <%end%>
  <%= yield %>
  </body>
</html>
