<%= form_for @referral_agency do |f| %>
  <%= f.label :email%>
  <%= f.text_field :email%>
  <%= f.label :password%>
  <%= f.text_field :password%>
  <%= f.label :name%>
  <%= f.text_field :name%>
  <%= f.label :address%>
  <%= f.text_field :address%>
  <%= f.label :phone%>
  <%= f.text_field :phone%>
  <%= f.submit %>
<% end %>