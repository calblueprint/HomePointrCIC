<h1>Landlords#show</h1>
<p>Find me in app/views/landlords/show.html.erb</p>

<div>
	<% if !@error %>
		<p> Property IDs: </p>
	  <% @properties.each do |property| %>
		  <p><%= property.id %></p>
		<% end %>
  <% else %>
  	<p> Error with Databsae </p>
  <% end %>
</div>

<% if user_signed_in? %>
  <li>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  </li>
<% else %>
  <li>
  <%= link_to('Login', new_user_session_path)  %>  
  </li>
<% end %>